{
  "functions": {
    "api/**/*": {
      "runtime": "nodejs14.x",
      "memory": 512
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "builds": [
    {
      "src": "api/**/*",
      "use": "@vercel/node"
    }
  ]
}
